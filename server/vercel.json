{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": { "maxLambdaSize": "50mb" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "env": {
        "SUPABASE_URL": "@supabase_url",
        "SUPABASE_KEY": "@supabase_key"
    }
    
}